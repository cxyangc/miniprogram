                <image class='profit_items_img' src='../../../images/myImg/after_sale2.png' style='width:86rpx;height:80rpx'></image>
                <text class='profit_items_text' style='margin-top:10px'>售后中</text>
              </view>
            </view>
        </view>
     
    </view>
