/* pages/contact/index.wxss */
.contactPage{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: small;
}
.button{
  margin-top: 20px;
  width: 8em;
  height: 8em;
  border-radius: 50%;
  background: green;
  color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}